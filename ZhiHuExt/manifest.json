{
  "manifest_version": 2,
  "name": "知乎疯牛病",
  "key": "",
  "version": "1.0.0",
  "author": "XZiar",
  "description": "知乎疯牛病插件————专治广告",
  "icons":
  {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "minimum_chrome_version": "55",
  "browser_action":
  {
    "default_icon": "icon.png",
    "default_title": "知乎疯牛病插件",
    "default_popup": "popup.html"
  },
  "permissions":
  [
    "*://www.zhihu.com/",
    "*://127.0.0.1:*/",
    "tabs",
    "storage",
    "downloads"
  ],
  "externally_connectable":
  {
    "matches":
    [
      "*://www.zhihu.com/*",
      "*://zhuanlan.zhihu.com/p/*"
    ]
  },
  "background":
  {
    "page": "background.html"
  },
  "content_scripts":
  [
    {
      "matches":
      [
        "*://www.zhihu.com/*",
        "*://zhuanlan.zhihu.com/*"
      ],
      "js":
      [
        "3rd/js/jquery-3.2.1.min.js",
        "pod.js",
        "utils.js",
        "ContentBase.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches":
      [
        "*://zhuanlan.zhihu.com/p/*"
      ],
      "js":
      [
        "ArticlePage.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches":
      [
        "*://www.zhihu.com/people/*",
        "*://www.zhihu.com/org/*"
      ],
      "js":
      [
        "PeoplePage.js"
      ],
      "css":
      [
        "cssfix.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches":
      [
        "*://www.zhihu.com/question/*"
      ],
      "js":
      [
        "QuestionPage.js"
      ],
      "css":
      [
        "cssfix.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches":
      [
        "*://www.zhihu.com/inbox/8912224000"
      ],
      "js":
      [
        "RepResPage.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches":
      [
        "*://www.zhihu.com/community/*"
      ],
      "exclude_matches":
      [
        "*://www.zhihu.com/community/reported/*"
      ],
      "js":
      [
        "CommunityRep.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches":
      [
        "*://www.zhihu.com/*"
      ],
      "exclude_matches":
      [
        "*://www.zhihu.com/inbox/8912224000"
      ],
      "js":
      [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
} 